{
  "name": "metalet",
  "private": true,
  "version": "2.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.dev.config.ts",
    "type-check": "vue-tsc --noEmit",
    "build:content": "vite build --config ./vite-config/vite.content.config.ts",
    "build:inject": "vite build --config ./vite-config/vite.inject.config.ts",
    "build:injectWatch": "vite build --config ./vite-config/vite.injectwatch.config.ts",
    "build:bg": "vite build --config ./vite-config/vite.background.config.ts",
    "build:web": "vue-tsc && vite build --config ./vite-config/vite.web.config.ts",
    "build": "set NODE_ENV=production && run-s clear build:content build:inject build:bg build:web",
    "b": "yarn build",
    "bw": "yarn build:web",
    "bwc": "yarn build:content --watch",
    "bwi": "yarn build:injectWatch",
    "bwb": "yarn build:bg --watch",
    "bww": "yarn build:web --watch",
    "clear": "rimraf dist"
  },
  "dependencies": {
    "@bitcoin-js/tiny-secp256k1-asmjs": "^2.2.3",
    "@headlessui/tailwindcss": "^0.1.3",
    "@headlessui/vue": "^1.7.12",
    "@heroicons/vue": "^2.0.16",
    "@rollup/plugin-alias": "^4.0.3",
    "@tanstack/vue-query": "^4.26.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/integrations": "^10.4.1",
    "axios": "0.27.2",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.3",
    "bitcore-lib": "^10.0.0",
    "bs58check": "^3.0.1",
    "buffer": "^6.0.3",
    "check-password-strength": "^2.0.7",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "decimal.js": "^10.4.3",
    "ecpair": "^2.1.0",
    "memfs": "^4.5.0",
    "meta-contract": "^0.3.29",
    "mvc-std-lib": "^1.1.4",
    "object-hash": "^3.0.0",
    "qrcode": "^1.5.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@types/bitcore-lib": "^0.15.3",
    "@types/chrome": "^0.0.228",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.15.10",
    "@types/object-hash": "^3.0.2",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.34.2",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.0.24",
    "webextension-polyfill": "^0.10.0"
  }
}
