{
  "name": "metalet",
  "private": true,
  "version": "1.7.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:content": "vite build --config vite.content.config.ts",
    "build:inject": "vite build --config vite.inject.config.ts",
    "build:injectWatch": "vite build --config vite.injectwatch.config.ts",
    "build:bg": "vite build --config vite.background.config.ts",
    "build:web": "vue-tsc && vite build",
    "build": "NODE_ENV=production run-s build:content build:inject build:bg build:web",
    "b": "yarn build",
    "bwc": "yarn build:content --watch",
    "bwi": "yarn build:injectWatch",
    "bwb": "yarn build:bg --watch",
    "bww": "yarn build:web --watch"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.1.3",
    "@headlessui/vue": "^1.7.12",
    "@heroicons/vue": "^2.0.16",
    "@rollup/plugin-alias": "^4.0.3",
    "@tanstack/vue-query": "^4.26.1",
    "@vueuse/core": "^10.3.0",
    "axios": "0.27.2",
    "bip32": "^4.0.0",
    "bitcoinjs-lib": "^6.1.3",
    "buffer": "^6.0.3",
    "check-password-strength": "^2.0.7",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "ecpair": "^2.1.0",
    "meta-contract": "0.3.29",
    "mvc-std-lib": "^1.1.4",
    "object-hash": "^3.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@types/chrome": "^0.0.228",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.15.10",
    "@types/object-hash": "^3.0.2",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.34.2",
    "autoprefixer": "^10.4.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "sass": "^1.62.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4",
    "vite": "^4.4.9",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.0.24",
    "webextension-polyfill": "^0.10.0"
